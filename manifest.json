{
	"manifest_version":2 ,
	"name": "TypeGuess",
	"version": "1",
	"permissions": [
      "storage"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
	"content_scripts": [
		{
		"matches": ["<all_urls>"],
		"js" : ["content.js"]
		}
	],
	"browser_action": {
		"default_icon" : "howl.png",
		"default_popup" : "popup.html",
		"default_title" : "Play A Game"
	}
}